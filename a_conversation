import os,shutil,json, pathlib
UNI="/home/ing/RICK/R_H_UNI"
CLEAN="/home/ing/RICK/RICK_LIVE_CLEAN"
rep=json.loads(pathlib.Path(f"{UNI}/logs/clean_diff_report.json").read_text())
