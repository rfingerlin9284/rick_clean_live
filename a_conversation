   # Before submitting any order:
   allowed, reason = mgr.pg_trade(symbol="EURUSD", side="buy", units=10000)
   if allowed:
       submit_order(symbol, side, units)
   else:
       log(f"Order blocked: {reason}")
   
   # Every tick/minute (call tick_enforce):
   actions = mgr.tick_enforce_positions()
