                    print(f'[{timestamp}] âŒ ERROR: {line.strip()}')
                else:
                    print(f'[{timestamp}] ðŸ“Š ACTIVITY: {line.strip()}')
            else:
                time.sleep(1)
except KeyboardInterrupt:
