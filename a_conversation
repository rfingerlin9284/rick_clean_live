Description=Emit multi-venue state/actions
[Service]
Type=oneshot
ExecStart=/bin/bash -lc '~/.local/bin/pg_now_all > ~/RICK/R_H_UNI/logs/actions_now_all.json.tmp && mv ~/RICK/R_H_UNI/logs/actions_now_all.json.tmp ~/RICK/R_H_UNI/logs/actions_now_all.json'
