    id="GBPUSD_1",
    symbol="GBPUSD",
    side="long",
    units=11200,
    entry_price=1.34038,
    current_price=1.34340,
    stop_loss=None,             # breakeven rule will set one
    opened_at=now - timedelta(hours=1, minutes=30),
    initial_sl=1.33839          # from panel line, if that was the SL plan
)
