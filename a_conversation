    id="USDCAD_1",
    symbol="USDCAD",
    side="short",
    units=10700,
    entry_price=1.40478,
    current_price=1.40559,
    stop_loss=None,             # hard SL absent -> time/BE logic still works
    opened_at=now - timedelta(hours=1, minutes=30),
