rfingerlin9284: #!/usr/bin/env bash
# RBOTzilla â€” Strategy/Guardian Docs Auditor & Sync
# Verifies documented files, copies them into RBOTzilla, locks them read-only,
# and emits a machine-readable index for the bot.

set -euo pipefail

SRC="/home/ing/RICK/RICK_LIVE_CLEAN"
DST="/home/ing/RICK/R_H_UNI/docs"
LOG="/home/ing/RICK/R_H_UNI/logs"
mkdir -p "$DST" "$LOG"

docs=(
  "STRATEGY_PARAMETERS_COMPLETE.md"
  "GUARDIAN_RULES_MATRIX.md"
  "CRITICAL_ISSUE_EMA_SCALPER_RR.md"
  "COMPLETE_STRATEGY_AUDIT_SUMMARY.md"
  "QUICK_REFERENCE_CHEAT_SHEET.md"
  "STRATEGY_ARCHITECTURE_DIAGRAM.md"
  "DOCUMENTATION_INDEX.md"
  "START_HERE.md"
  "FINAL_SUMMARY_REQUEST_COMPLETE.md"
)

INDEX_JSON="$DST/index.json"
TMP_JSON="$(mktemp)"

printf "%-40s | %-8s | %-10s | %-20s | %-12s\n" "FILE" "STATE" "SIZE" "MTIME(UTC)" "SHA256"
printf -- "---------------------------------------------------------------------------------------------------------------\n" 

echo "[" > "$TMP_JSON"
first=1
for f in "${docs[@]}"; do
  srcf="$SRC/$f"
  dstf="$DST/$f"
  if [[ -f "$srcf" ]]; then
    # copy & lock
    install -m 444 "$srcf" "$dstf"
    # gather facts
    size=$(stat -c '%s' "$dstf")
    mtime=$(date -u -d @"$(stat -c '%Y' "$dstf")" '+%Y-%m-%dT%H:%M:%SZ')
    sha=$(sha256sum "$dstf" | awk '{print $1}')
    state="OK"
  else
    size=0; mtime=""; sha=""; state="MISSING"
  fi

  printf "%-40s | %-8s | %-10s | %-20s | %-12s\n" "$f" "$state" "$size" "$mtime" "${sha:0:12}"

  # write JSON row
  row=$(jq -n \
    --arg name "$f" \
    --arg state "$state" \
    --arg mtime "$mtime" \
    --arg sha "$sha" \
    --arg src "$srcf" \
    --arg dst "$dstf" \
    --arg size "$size" \
    '{file:$name,state:$state,mtime:$mtime,sha256:$sha,src:$src,dst:$dst,size:($size|tonumber)}')

  if [[ $first -eq 1 ]]; then
    echo "  $row" >> "$TMP_JSON"; first=0
  else
    echo " ,$row" >> "$TMP_JSON"
  fi
done
echo "]" >> "$TMP_JSON"
mv "$TMP_JSON" "$INDEX_JSON"

echo
echo "[OK] Audit complete."
echo "Index: $INDEX_JSON"
echo "Preview:"
jq '.[] | {file,state,size,mtime,sha256: (.sha256[0:12])}' "$INDEX_JSON" | tee "$LOG/docs_audit.log"



