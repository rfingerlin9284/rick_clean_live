Ran terminal command: python3 << 'OCO_TEST'
import os
os.environ['TRADING_MODE'] = 'CANARY'

from brokers.oanda_connector import OandaConnector

print("ðŸ“Š SENDING OCO TEST TRADES - OANDA PAPER")
print("=" * 80)

oanda = OandaConnector()

# Test 1: EUR/USD
print("\n1ï¸âƒ£  EUR/USD - OCO BUY")
try:
    result = oanda.place_oco_order(
        instrument="EUR_USD",
        entry_price=1.0950,
        stop_loss=1.0900,
        take_profit=1.1000,
        units=1000
    )
    if result.get('success'):
        print(f"   âœ… SUCCESS - Trade placed")
        print(f"   Order ID: {result.get('orderId', 'N/A')}")
    else:
        print(f"   âš ï¸  {result.get('error', 'Unknown error')}")
except Exception as e:
    print(f"   âŒ Error: {e}")

# Test 2: GBP/USD
print("\n2ï¸âƒ£  GBP/USD - OCO SELL")
try:
    result = oanda.place_oco_order(
        instrument="GBP_USD",
        entry_price=1.2700,
        stop_loss=1.2750,
        take_profit=1.2650,
        units=-500
    )
    if result.get('success'):
        print(f"   âœ… SUCCESS - Trade placed")
        print(f"   Order ID: {result.get('orderId', 'N/A')}")
    else:
        print(f"   âš ï¸  {result.get('error', 'Unknown error')}")
except Exception as e:
    print(f"   âŒ Error: {e}")

# Test 3: XAU/USD
print("\n3ï¸âƒ£  XAU/USD - OCO BUY")
try:
    result = oanda.place_oco_order(
        instrument="XAU_USD",
        entry_price=2000,
        stop_loss=1980,
        take_profit=2020,
        units=10
    )
    if result.get('success'):
        print(f"   âœ… SUCCESS - Trade placed")
        print(f"   Order ID: {result.get('orderId', 'N/A')}")
    else:
        print(f"   âš ï¸  {result.get('error', 'Unknown error')}")
except Exception as e:
    print(f"   âŒ Error: {e}")

print("\n" + "=" * 80)
